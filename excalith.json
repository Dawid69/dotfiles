		"titleColor": "yellow"
	},
	"urlLaunch": {
		"target": "_self",
		"defaultColor": "white",
		"hoverColor": "violet"
	},
	"search": {
		"default": "https://google.com/search?q=",
		"target": "_self",
		"shortcutRegex": "([A-Za-z0-9]+) (.*)",
